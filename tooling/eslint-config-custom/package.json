        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-turbo": "^2.1.3",
        "typescript-eslint": "^8.9.0"
    },
    "devDependencies": {
        "@vercel/style-guide": "^6.0.0",
        "@saroh/tsconfig": "workspace:*",
        "@types/eslint__js": "8.42.3",
